#include <stdio.h>
#include <string.h>

typedef struct{
    int x;
    int y;
} Ponto;


#define QUANTIDADE_DE_PONTOS 3

int calculadist(int x, int y){
    int dist
    dist = (p[0].x - p[1].x) + (p[0].y - p[1].y))
}
int main() {
    
    
    
    Ponto p[QUANTIDADE_DE_PONTOS];
    
    for (int i = 0; i < QUANTIDADE_DE_PONTOS; i++){
        printf("Ponto %d: ", i);
        scanf("%d", p[i].x);
        scanf("%d", p[i].y);
        
    }
    

    return 0;
}
