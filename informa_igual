    for(i=0;i<6;i++){
        printf("Informe um numero: \n");
        scanf("%lf", &vet[i]);
       vet2[i]=vet[i];
    }
    
    for(i=0;i<3;i++){
        for(r=3;r<6;r++){
            vet[i]==vet2[r];
            if(vet[i]==vet2){
                
            }
        }
    }
    
